{
  "compilerOptions": {
      "target": "es2020",
      "module": "es2020",
      "rootDir": "./src",
      // "types": ["@peeky/test"],
      "sourceMap": true,
      "outDir": "./dist",
      "moduleResolution": "bundler",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "allowImportingTsExtensions": true,
      "strict": true,
      "skipLibCheck": true
  }
}
